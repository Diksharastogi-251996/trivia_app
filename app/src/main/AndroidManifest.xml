<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sagarkhurana.quizenew">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizForFun">
        <activity android:name="newquizeapp.splashactivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="newquizeapp.quizactivity" />
        <activity android:name="newquizeapp.FinalResultActivity" />

        <activity android:name="newquizeapp.HistoryActivity" />

        <activity android:name="newquizeapp.RegisterActivity" />
        <activity
            android:name="newquizeapp.LoginActivity"
            android:theme="@style/splashScreenTheme">

        </activity>
        <activity android:name="newquizeapp.MainActivity" />
    </application>

</manifest>